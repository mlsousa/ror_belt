

<div id="container">
  <h5><a href="/users/<%= current_user.id %>">My Account</a></h5>
	<h2><%=@song.title%></h2>


	<table class="table table-striped table-bordered">
		<thead>
			<tr>
				<td><h5>Name</h5></td>
				<td><h5>Added 'x' Times</h5></td>
			</tr>
		</thead>
		<tbody>
			<% @users.each do |user| %>


				<tr>
					<td><h6><a href="/users/<%= user.id %>" value="<%=user.id%>" name="user[:user_id]"><%= user.first_name %> <%= user.last_name %></a></h6></td>
					<td><h6><%= user.playlist.songs.count %></h6></td>

				</tr>
			<% end %>
		</tbody>
	</table>
</div>
